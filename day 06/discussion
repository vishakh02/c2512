SQL> 
  
  CREATE TABLE countries (
        id NUMBER GENERATED BY DEFAULT AS IDENTITY (START WITH 11 INCREMENT BY 10) PRIMARY KEY,
        name VARCHAR2(125) NOT NULL,
        ruler VARCHAR2(255) NOT NULL
    );

Table created.

SQL> insert into country(id, name, ruler) values(11, 'USA', 'Trump');
insert into country(id, name, ruler) values(11, 'USA', 'Trump')
            *
ERROR at line 1:
ORA-00942: table or view does not exist


SQL> insert into countries(id, name, ruler) values(11, 'USA', 'Trump');

1 row created.

SQL> select * from countries;

        ID NAME                           RULER
---------- ------------------------------ ---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
        11 USA                            Trump

SQL> insert into countries(id, name, ruler) values(11, 'India', 'Modi');
insert into countries(id, name, ruler) values(11, 'India', 'Modi')
*
ERROR at line 1:
ORA-00001: unique constraint (C##EMS.SYS_C008320) violated


SQL> insert into countries(id, name, ruler) values(21, 'India', 'Modi');

1 row created.

SQL> select * from countries;

        ID NAME                           RULER
---------- ------------------------------ ---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
        11 USA                            Trump
        21 India                          Modi

